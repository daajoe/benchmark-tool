bgen runscript1.xml
bgen runscript2.xml
./output/project1/machineX/start.sh
./output/project2/machineY/start.sh
beval runscript1.xml > result1.xml
beval runscript2.xml > result2.xml
bclean runscript1.xml
bclean runscript2.xml
# additional selections might be interesting
bmerge result1.xml result2.xml > result3.xml
btrans --soffice result3.xml > result3.odf
btrans --csv result3.xml > result3.csv

