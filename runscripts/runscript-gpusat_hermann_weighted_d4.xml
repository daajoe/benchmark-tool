<runscript output="output">

	<machine name="hermann" cpu="8xE5520@2.27GHz" memory="12GB"/>

	<config name="seq-gpusat-prim" template="templates/seq-gpusat-prim.sh"/>
	<config name="seq-gpusat-inci" template="templates/seq-gpusat-inci.sh"/>
	
	<system name="gpusat_d4_p" measures="gpusat" version="0" config="seq-gpusat-prim">
		<setting name="prim_weighted_d4"  cmdline="-c /home/mzisser/benchmark-tool/programs/kernel/ -w 14 -m 24 --weighted "/>
	</system>
	
	<system name="gpusat_d4_i" measures="gpusat" version="0" config="seq-gpusat-inci">
		<setting name="inci_weighted_d4"  cmdline="-c /home/mzisser/benchmark-tool/programs/kernel/ -w 14 -m 24 --weighted "/>
	</system>

	<seqjob name="seq-gpusat" timeout="900" runs="1" parallel="1"/>

	<benchmark name="weighted">
		<folder path="/files/mzisser/Benchmarks/weighted_new"/>
	</benchmark>

	<project name="gpusat_hermann_weighted_d4" job="seq-gpusat">
		<runtag machine="hermann" benchmark="weighted" tag="*all*"/>
	</project>

</runscript>

