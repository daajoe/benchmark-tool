	<machine name="cobra" cpu="24x8xE5520@2.27GHz" memory="24GB"/>

	<config name="condor-generic" template="templates/condor/runsolver_template.py"/>
	<system name="dynaspfast" version="2.6" measures="asp" config="condor-generic">
 	  <setting name="dynaspfast_n_c2_l2_y_r_1580" tag="asp" cmdline="-n -c 2 -l 2 -y -r -bb -s 1580" ppn="4" procs="1"/>		
 	  <setting name="dynaspfast_n_c2_l2_y_1580" tag="asp" cmdline="-n -c 2 -l 2 -y -bb -s 1580" ppn="4" procs="1"/>		
		<setting name="dynaspfast_n_c2_l2_y_r_11680" tag="asp" cmdline="-n -c 2 -l 2 -y -r -bb -s 11680" ppn="4" procs="1"/>		
 	  <setting name="dynaspfast_n_c2_l2_y_11680" tag="asp" cmdline="-n -c 2 -l 2 -y -bb -s 11680" ppn="4" procs="1"/>		
	  <!--<setting name="glucose-syrup" tag="sat" cmdline="" ppn="4" procs="1"/>-->
 	</system>
	
	<system name="dynaspold" version="2.0" measures="asp" config="condor-generic">
 	  <setting name="dynaspold_c2_1580" tag="asp" cmdline="-c 2 -s 1580 -bb" ppn="4" procs="1"/>		
 	  <setting name="dynaspold_c2_11680" tag="asp" cmdline="-c 2 -s 11680 -bb" ppn="4" procs="1"/>		
		<setting name="dynaspold_c6_1580" tag="asp" cmdline="-c 6 -s 1580 -bb" ppn="4" procs="1"/>		
 	  <setting name="dynaspold_c6_11680" tag="asp" cmdline="-c 6 -s 11680 -bb" ppn="4" procs="1"/>		
	  <!--<setting name="glucose-syrup" tag="sat" cmdline="" ppn="4" procs="1"/>-->
 	</system>

	<system name="dynasp" version="2.5" measures="asp" config="condor-generic">
 	  <setting name="dynasp" tag="asp" cmdline="" ppn="4" procs="1"/>		
	  <!--<setting name="glucose-syrup" tag="sat" cmdline="" ppn="4" procs="1"/>-->
 	</system>
	<system name="clasp" version="3.0" measures="asp" config="condor-generic">
 	  <setting name="clasp" tag="asp" cmdline="--stats=2 --opt-strategy=usc,pmres,disjoint,stratify --opt-usc-shrink=min -q" ppn="4" procs="1"/>		
	</system>
