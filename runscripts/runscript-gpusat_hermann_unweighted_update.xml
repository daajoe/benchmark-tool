<runscript output="output">

	<machine name="hermann" cpu="8xE5520@2.27GHz" memory="12GB"/>

	<config name="seq-gpusat-prim" template="templates/seq-gpusat-prim.sh"/>
	<config name="seq-gpusat-inci" template="templates/seq-gpusat-inci.sh"/>
	
	<system name="gpusat_p_update" measures="gpusat" version="0" config="seq-gpusat-prim">
		<setting name="prim_unweighted_update"  cmdline="-c /home/mzisser/benchmark-tool/programs/kernel/ -w 14 -m 26"/>
	</system>
	
	<system name="gpusat_i_update" measures="gpusat" version="0" config="seq-gpusat-inci">
		<setting name="inci_unweighted_update"  cmdline="-c /home/mzisser/benchmark-tool/programs/kernel/ -w 14 -m 26"/>
	</system>

	<seqjob name="seq-gpusat" timeout="900" runs="1" parallel="1"/>

	<benchmark name="unweighted">
		<folder path="/files/mzisser/Benchmarks/unweighted_new"/>
	</benchmark>

	<project name="gpusat_hermann_unweighted_update" job="seq-gpusat">
		<runtag machine="hermann" benchmark="unweighted" tag="*all*"/>
	</project>

</runscript>

