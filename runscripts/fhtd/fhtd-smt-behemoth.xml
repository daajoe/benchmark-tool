<runscript output="output">

    <machine name="behemoth" cpu="24x8xE5520@2.27GHz" memory="8GB"/>

    <config name="pbs-generic" template="templates/runsolver_template.py"/>
    <!--templates/runsolver_template.py-->
    <!--TODO: measures-->

    <system name="fhtd" version="0.7" measures="fhtd" config="pbs-generic">
      <setting name="sat" tag="sat" cmdline="-ncb -ntb -np" ppn="1" procs="1" pbstemplate="templates/pbs/single-behemoth-fhtd.pbs"/>
      <setting name="sat-clique4" tag="sat" cmdline="-ntb -np" ppn="1" procs="1" pbstemplate="templates/pbs/single-behemoth-fhtd.pbs"/>
      <setting name="sat-twin" tag="sat" cmdline="-ntb -np" ppn="1" procs="1" pbstemplate="templates/pbs/single-behemoth-fhtd.pbs"/>
      <setting name="sat-pre-clique4" tag="sat" cmdline="-ntb" ppn="1" procs="1" pbstemplate="templates/pbs/single-behemoth-fhtd.pbs"/>
      <setting name="sat-pre-twin" tag="sat" cmdline="-ntb" ppn="1" procs="1" pbstemplate="templates/pbs/single-behemoth-fhtd.pbs"/>
      <setting name="sat-pre-clique3-twin" tag="sat" cmdline="-ck=3" ppn="1" procs="1" pbstemplate="templates/pbs/single-behemoth-fhtd.pbs"/>
      <setting name="sat-pre-clique4-twin" tag="sat" cmdline="-ck=4" ppn="1" procs="1" pbstemplate="templates/pbs/single-behemoth-fhtd.pbs"/>
      <setting name="sat-pre-clique6-twin" tag="sat" cmdline="-ck=6" ppn="1" procs="1" pbstemplate="templates/pbs/single-behemoth-fhtd.pbs"/>
      <setting name="sat-pre-clique8-twin" tag="sat" cmdline="-ck=8" ppn="1" procs="1" pbstemplate="templates/pbs/single-behemoth-fhtd.pbs"/>
    </system>
    <system name="detkdecomp-prog" version="1.0" measures="detkdecomp" config="pbs-generic">
        <setting name="default" tag="comb" cmdline="50" ppn="1" procs="1" pbstemplate="templates/pbs/single-behemoth-fhtd.pbs"/>
    </system>

    <!-- todo condor job priority -->
    <!--<condorjob name="condor-generic" memout="4096" timeout="7200" runs="1" script_mode="timeout" walltime="23:59:59" condortemplate="templates/condor/condor.submit" basedir="/home/jfichte/src/benchmark-tool/"/>-->
    <!--<condorjob name="condor-generic" memout="2048" timeout="900" runs="1" script_mode="timeout" walltime="23:59:59" condortemplate="templates/condor/condor.submit" basedir="/Users/fichte/decodyn/src/benchmark-tool
"/>-->
    <pbsjob name="pbs-generic" timeout="7200" memout="8192" runs="1" script_mode="timeout" walltime="2:30:00"/>


    <benchmark name="hyperbench-cq">
        <folder path="benchmarks/hypergraphs/hyperbench/cq">
            <ignore prefix="."/>
        </folder>
    </benchmark>


    <benchmark name="hyperbench-csp_application">
        <folder path="benchmarks/hypergraphs/hyperbench/csp_application">
            <ignore prefix="."/>
        </folder>
    </benchmark>

    <benchmark name="hyperbench-csp_other">
        <folder path="benchmarks/hypergraphs/hyperbench/csp_other">
            <ignore prefix="."/>
        </folder>
    </benchmark>

    <benchmark name="hyperbench-csp_random">
        <folder path="benchmarks/hypergraphs/hyperbench/csp_random">
            <ignore prefix="."/>
        </folder>
    </benchmark>


    <benchmark name="hyperbench-daimlerchrysler">
        <folder path="benchmarks/hypergraphs/hyperbench/daimlerchrysler">
            <ignore prefix="."/>
        </folder>
    </benchmark>

    <benchmark name="hyperbench-grid2d">
        <folder path="benchmarks/hypergraphs/hyperbench/grid2d">
            <ignore prefix="."/>
        </folder>
    </benchmark>


    <benchmark name="hyperbench-iscas89">
        <folder path="benchmarks/hypergraphs/hyperbench/iscas89">
            <ignore prefix="."/>
        </folder>
    </benchmark>


    <benchmark name="hyperbench-maxsat">
        <folder path="benchmarks/hypergraphs/hyperbench/maxsat">
            <ignore prefix="."/>
        </folder>
    </benchmark>


    <benchmark name="hyperbench-other">
        <folder path="benchmarks/hypergraphs/hyperbench/other">
            <ignore prefix="."/>
        </folder>
    </benchmark>


    <project name="fhtd-hypergraphs" job="pbs-generic">
      <!--
	<runtag machine="behemoth" benchmark="hyperbench-cq" tag="comb"/>
        <runtag machine="behemoth" benchmark="hyperbench-csp_application" tag="comb"/>
        <runtag machine="behemoth" benchmark="hyperbench-csp_other" tag="comb"/>
        <runtag machine="behemoth" benchmark="hyperbench-csp_random" tag="comb"/>
        <runtag machine="behemoth" benchmark="hyperbench-daimlerchrysler" tag="comb"/>
        <runtag machine="behemoth" benchmark="hyperbench-grid2d" tag="comb"/>
        <runtag machine="behemoth" benchmark="hyperbench-iscas89" tag="comb"/>
        <runtag machine="behemoth" benchmark="hyperbench-maxsat" tag="comb"/>
        <runtag machine="behemoth" benchmark="hyperbench-other" tag="comb"/>
	-->
        <runtag machine="behemoth" benchmark="hyperbench-cq" tag="*all*"/>
        <runtag machine="behemoth" benchmark="hyperbench-csp_application" tag="*all*"/>
        <runtag machine="behemoth" benchmark="hyperbench-csp_other" tag="*all*"/>
        <runtag machine="behemoth" benchmark="hyperbench-csp_random" tag="*all*"/>
        <runtag machine="behemoth" benchmark="hyperbench-daimlerchrysler" tag="*all*"/>
        <runtag machine="behemoth" benchmark="hyperbench-grid2d" tag="*all*"/>
        <runtag machine="behemoth" benchmark="hyperbench-iscas89" tag="*all*"/>
        <runtag machine="behemoth" benchmark="hyperbench-maxsat" tag="*all*"/>
        <runtag machine="behemoth" benchmark="hyperbench-other" tag="*all*"/>
    </project>
</runscript>
