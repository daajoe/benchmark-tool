#!/usr/bin/python
import sys
import os.path
import random

f = file(sys.argv[1], "r")
proj = []
remainder = []
for r in f:
	if r.startswith("c ind"):
		var = r.split(" ")[2:-1]
		proj += var
	elif not r.startswith("c"):
		remainder.append(r[:-1])
f.close()
#print proj
print("c ind {} 0".format(" ".join(proj)))
for i in remainder:
	print(i)
